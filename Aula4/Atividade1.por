programa
{
	
	funcao inicio()
	{
		
		// Escreva um algoritmo em PORTUGOL que leia um número e o imprima caso ele seja maior que 20

		inteiro numero

		escreva ("Informe um numero: ")
		leia (numero)

		se ( numero >= 21 )
		escreva ("O numero informado é Maior que 20")

		senao
		escreva ("O numero informado é Menor que 20")
		
		
	}
}
